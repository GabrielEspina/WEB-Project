<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp">
	<display-name>JavaTango - Web 2.0 - Framework </display-name>


 	<servlet>
		<servlet-name>Demo</servlet-name>
		<display-name>DemoServlet</display-name>
		<servlet-class>com.ar.itr.demo.DemoServ</servlet-class>
		
	</servlet>
 
  
  <!--   mapping  -->
 

	<servlet-mapping>
		<servlet-name>Demo</servlet-name>
		<url-pattern>/demo</url-pattern>
	</servlet-mapping>

	<!--   vida de la session en minutos  -->
  <session-config>
    <session-timeout>480</session-timeout>
  </session-config>

  
	<welcome-file-list>
		<!-- <welcome-file>/test.jsp</welcome-file> --> 
		<welcome-file>/index.html</welcome-file>
		
 		<!-- <welcome-file>jsp/include/test_02.jsp</welcome-file>  --> 
	</welcome-file-list>
	
 
 
</web-app>
